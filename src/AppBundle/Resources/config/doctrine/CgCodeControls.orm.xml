<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CgCodeControls" table="CG_CODE_CONTROLS">
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="ccComment" type="string" column="CC_COMMENT" length="240" nullable="true"/>
    <field name="ccDomain" type="string" column="CC_DOMAIN" length="30" nullable="false"/>
    <field name="ccIncrement" type="integer" column="CC_INCREMENT" nullable="true"/>
    <field name="ccNextValue" type="integer" column="CC_NEXT_VALUE" nullable="false"/>
  </entity>
</doctrine-mapping>
