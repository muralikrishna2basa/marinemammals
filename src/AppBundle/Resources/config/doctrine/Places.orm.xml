<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Places" table="PLACES">
    <indexes>
      <index name="IDX_E57ABD3EE3C1733" columns="PCE_SEQNO"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="pl_uk_name" columns="NAME"/>
    </unique-constraints>
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="name" type="string" column="NAME" length="50" nullable="true"/>
    <field name="type" type="string" column="TYPE" length="20" nullable="true"/>
    <many-to-one field="pceSeqno" target-entity="Places">
      <join-columns>
        <join-column name="PCE_SEQNO" referenced-column-name="SEQNO"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
