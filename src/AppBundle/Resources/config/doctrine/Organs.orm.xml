<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Organs" table="ORGANS">
    <indexes>
      <index name="ogn_ogn_fk_i" columns="OGN_CODE"/>
    </indexes>
    <id name="code" type="string" column="CODE" length="10">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="name" type="string" column="NAME" length="50" nullable="false"/>
    <many-to-one field="ognCode" target-entity="Organs">
      <join-columns>
        <join-column name="OGN_CODE" referenced-column-name="CODE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
