<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Signup" table="SIGNUP">
    <unique-constraints>
      <unique-constraint name="signup__login_uk" columns="LOGIN"/>
      <unique-constraint name="signup_confirm_code_uk" columns="CONFIRM_CODE"/>
      <unique-constraint name="signup_email_uk" columns="EMAIL"/>
    </unique-constraints>
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="address" type="string" column="ADDRESS" length="250" nullable="true"/>
    <field name="confirmCode" type="string" column="CONFIRM_CODE" length="40" nullable="true"/>
    <field name="created" type="integer" column="CREATED" nullable="true"/>
    <field name="email" type="string" column="EMAIL" length="50" nullable="true"/>
    <field name="firstName" type="string" column="FIRST_NAME" length="50" nullable="true"/>
    <field name="lastName" type="string" column="LAST_NAME" length="50" nullable="true"/>
    <field name="login" type="string" column="LOGIN" length="50" nullable="false"/>
    <field name="password" type="string" column="PASSWORD" length="50" nullable="false"/>
    <field name="phoneNumber" type="string" column="PHONE_NUMBER" length="20" nullable="true"/>
    <field name="sex" type="string" column="SEX" length="1" nullable="true"/>
    <field name="title" type="string" column="TITLE" length="5" nullable="true"/>
  </entity>
</doctrine-mapping>
