<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CgRefCodes" table="CG_REF_CODES">
    <indexes>
      <index name="x_cg_ref_codes_1" columns="RV_DOMAIN,RV_LOW_VALUE"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="cg_ref_codes_rv_meaning_uk" columns="RV_MEANING"/>
    </unique-constraints>
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="rvAbbreviation" type="string" column="RV_ABBREVIATION" length="240" nullable="true"/>
    <field name="rvDomain" type="string" column="RV_DOMAIN" length="100" nullable="false"/>
    <field name="rvHighValue" type="string" column="RV_HIGH_VALUE" length="240" nullable="true"/>
    <field name="rvLowValue" type="string" column="RV_LOW_VALUE" length="240" nullable="false"/>
    <field name="rvMeaning" type="string" column="RV_MEANING" length="240" nullable="true"/>
  </entity>
</doctrine-mapping>
