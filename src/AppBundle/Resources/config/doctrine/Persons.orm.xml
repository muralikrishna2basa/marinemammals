<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Persons" table="PERSONS">
    <indexes>
      <index name="psn_ite_fk_i" columns="ITE_SEQNO"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="psn_uk" columns="FIRST_NAME,LAST_NAME"/>
      <unique-constraint name="psn_email_uk" columns="EMAIL"/>
    </unique-constraints>
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="address" type="string" column="ADDRESS" length="250" nullable="true"/>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="email" type="string" column="EMAIL" length="80" nullable="true"/>
    <field name="firstName" type="string" column="FIRST_NAME" length="30" nullable="true"/>
    <field name="idodId" type="integer" column="IDOD_ID" nullable="true"/>
    <field name="lastName" type="string" column="LAST_NAME" length="30" nullable="true"/>
    <field name="loginName" type="string" column="LOGIN_NAME" length="25" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="password" type="string" column="PASSWORD" length="60" nullable="true"/>
    <field name="phoneNumber" type="string" column="PHONE_NUMBER" length="20" nullable="true"/>
    <field name="pic" type="string" column="PIC" length="100" nullable="true"/>
    <field name="sessionid" type="string" column="SESSIONID" length="32" nullable="true"/>
    <field name="sex" type="string" column="SEX" length="50" nullable="true"/>
    <field name="title" type="string" column="TITLE" length="50" nullable="true"/>
    <many-to-one field="iteSeqno" target-entity="Institutes">
      <join-columns>
        <join-column name="ITE_SEQNO" referenced-column-name="SEQNO"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="rlnSeqno" target-entity="RequestLoans" mapped-by="psnSeqno"/>
    <many-to-many field="grpName" target-entity="Groups" mapped-by="psnSeqno"/>
  </entity>
</doctrine-mapping>
