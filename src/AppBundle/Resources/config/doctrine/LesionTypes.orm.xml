<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\LesionTypes" table="LESION_TYPES">
    <indexes>
      <index name="IDX_B8A6D97EFBE675E9" columns="OGN_CODE"/>
    </indexes>
    <id name="seqno" type="integer" column="SEQNO"/>
    <id name="ognCode" association-key="true"/>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="name" type="string" column="NAME" length="3" nullable="true"/>
    <field name="processus" type="string" column="PROCESSUS" length="4" nullable="false"/>
    <one-to-one field="ognCode" target-entity="Organs">
      <join-columns>
        <join-column name="OGN_CODE" referenced-column-name="CODE"/>
      </join-columns>
    </one-to-one>
    <many-to-many field="ncyEseSeqno" target-entity="Necropsies" mapped-by="lteOgnCode"/>
  </entity>
</doctrine-mapping>
