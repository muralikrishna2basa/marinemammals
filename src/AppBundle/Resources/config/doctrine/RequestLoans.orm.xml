<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RequestLoans" table="REQUEST_LOANS">
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="dateOut" type="datetime" column="DATE_OUT" nullable="true"/>
    <field name="dateRequest" type="datetime" column="DATE_REQUEST" nullable="false"/>
    <field name="dateRt" type="datetime" column="DATE_RT" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="status" type="integer" column="STATUS" nullable="false"/>
    <field name="studyDescription" type="string" column="STUDY_DESCRIPTION" length="250" nullable="false"/>
    <many-to-many field="speSeqno" target-entity="Samples" mapped-by="rlnSeqno"/>
    <many-to-many field="psnSeqno" target-entity="Persons" mapped-by="rlnSeqno"/>
  </entity>
</doctrine-mapping>
