<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Groups" table="GROUPS">
    <id name="name" type="string" column="NAME" length="30">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="accessLevel" type="integer" column="ACCESS_LEVEL" nullable="false"/>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="description" type="string" column="DESCRIPTION" length="4000" nullable="false"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <many-to-many field="psnSeqno" target-entity="Persons" mapped-by="grpName"/>
    <many-to-many field="pmdSeqno" target-entity="ParameterMethods" mapped-by="grpName"/>
    <many-to-many field="mnuSeqno" target-entity="Menus" mapped-by="grpName"/>
  </entity>
</doctrine-mapping>
