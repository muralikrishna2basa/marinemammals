<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Menus" table="MENUS">
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="description" type="string" column="DESCRIPTION" length="400" nullable="true"/>
    <field name="display" type="string" column="DISPLAY" length="1" nullable="true"/>
    <field name="displayOrder" type="integer" column="DISPLAY_ORDER" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="name" type="string" column="NAME" length="200" nullable="false"/>
    <field name="script" type="string" column="SCRIPT" length="100" nullable="false"/>
    <many-to-many field="grpName" target-entity="Groups" mapped-by="mnuSeqno"/>
  </entity>
</doctrine-mapping>
