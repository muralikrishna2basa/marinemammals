<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Samples" table="SAMPLES">
    <indexes>
      <index name="spe_cln_fk_i" columns="CLN_SEQNO"/>
    </indexes>
    <id name="seqno" type="integer" column="SEQNO">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="analyzeDest" type="string" column="ANALYZE_DEST" length="50" nullable="false"/>
    <field name="availability" type="string" column="AVAILABILITY" length="50" nullable="true"/>
    <field name="conservationMode" type="string" column="CONSERVATION_MODE" length="50" nullable="false"/>
    <field name="creDat" type="datetime" column="CRE_DAT" nullable="true"/>
    <field name="creUser" type="string" column="CRE_USER" length="30" nullable="true"/>
    <field name="modDat" type="datetime" column="MOD_DAT" nullable="true"/>
    <field name="modUser" type="string" column="MOD_USER" length="30" nullable="true"/>
    <field name="speType" type="string" column="SPE_TYPE" length="50" nullable="false"/>
    <field name="subref" type="string" column="SUBREF" length="10" nullable="true"/>
    <many-to-one field="clnSeqno" target-entity="ContainerLocalizations">
      <join-columns>
        <join-column name="CLN_SEQNO" referenced-column-name="SEQNO"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="olnLteSeqno" target-entity="OrganLesions" mapped-by="speSeqno"/>
    <many-to-many field="rlnSeqno" target-entity="RequestLoans" mapped-by="speSeqno"/>
  </entity>
</doctrine-mapping>
