{% extends 'AppBundle::nocol-layout.html.twig' %}
 {% block title %}observation{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    '@AppBundle/Resources/public/css/font-awesome.css'
    'http://cdn.jsdelivr.net/qtip2/2.2.1/jquery.qtip.min.css'%}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'http://cdn.jsdelivr.net/qtip2/2.2.1/jquery.qtip.min.js' %}
    <script type="application/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="application/javascript">
        $(document).ready(function () {
            $('[data-tooltip!=""]').qtip({
                content: {
                    attr: 'data-tooltip'
                }
            });
        });
    </script>
{% endblock %}

{% block main_content %}
    <div class="col-lg-12">
        {{ include('AppBundle:Bare:mgmt-view-observations-specimens.html.twig') }}
    </div>
{% endblock %}

