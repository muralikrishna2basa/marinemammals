<h2>Add new source</h2>
{{ form_start(form, {'method': 'POST','action': path('mm_sources_add_create')}) }}
<ul class="formelements">
    <li>{{ form_row(form.name, {'label':'Name'}) }}</li>
    <li>{{ form_row(form.type, {'label':'Source type'}) }}</li>
</ul>
<p>
    <input type="submit" value="Submit">
</p>
{{ form_end(form) }}