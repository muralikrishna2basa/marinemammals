<h2>Add new source</h2>
{{ form_start(form, {'method': 'POST','action': path('mm_sources_add_create')}) }}
<fieldset>
    <div class="form-inline">{{ form_row(form.name, {'label':'Name'}) }}</div>
    <div class="form-inline">{{ form_row(form.type, {'label':'Source type'}) }}</div>
</fieldset>
<p>
    <input type="submit" value="Submit">
</p>
{{ form_end(form) }}