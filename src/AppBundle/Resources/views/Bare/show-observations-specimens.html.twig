<h2>{{ observation.eseSeqno.spec2events.scnSeqno.txnSeqno.vernacularNameEn|capitalize }} -
    <i>{{ observation.eseSeqno.spec2events.scnSeqno.txnSeqno.canonicalName }}</i></h2>
<ul>{% if observation.isconfidential %}
        <li><span class="confidential_observation">Confidential observation</span></li>{% endif %}
    {% if observation.samplingeffort %}
        <li><span>{{ observation.samplingeffort }}</span></li>{% endif %}
    {% if observation.cpnCode %}
        <li><span>Campaign: {{ observation.cpnCode }}</span></li>{% endif %}
</ul>

<table class="observation_basic_view table">
    <tbody>
    <tr>
        <td>Date</td>
        <td>{{ observation.eseSeqno.eventDatetime.date }}</td>
        <td>{{ observation.eseSeqno.eventDatetimeFlag }}</td>
    </tr>
    <tr>
        <td>Number</td>
        <td>{{ observation.eseSeqno.spec2events.scnSeqno.scnNumber }} {{ observation.eseSeqno.spec2events.scnSeqno.sex }} {{ observation.osnType }}</td>
    </tr>
    <tr>
        <td>Identification is certain?</td>
        <td>{{ observation.eseSeqno.spec2events.scnSeqno.identificationCertainty }}</td>
    </tr>
    <tr>
        <td>Location</td>
        <td>{{ observation.stnSeqno.description }}</td>
        <td>{{ observation.precisionFlag }}</td>
    </tr>
    <tr>
        <td>Decimal latitude</td>
        <td>{{ observation.latDec }}</td>
    </tr>
    <tr>
        <td>Decimal longitude</td>
        <td>{{ observation.lonDec }}</td>
    </tr>


    <tr>
        <td>Platform</td>
        <td>{{ observation.pfmSeqno }}</td>
    </tr>
    <tr>
        <td>RBINS tag</td>
        <td>{{ observation.eseSeqno.spec2events.scnSeqno.rbinsTag }}</td>
    </tr>
    <tr>
        <td>Necropsy tag</td>
        <td>{{ observation.eseSeqno.spec2events.scnSeqno.necropsyTag }}</td>
    </tr>
    <tr>
        <td>Source</td>
        <td>{{ observation.singleSource }}</td>
    </tr>
    {#
   <tr>
        <td>Person</td>
        <td>{{ e2p_informer.psnSeqno }}</td>
    </tr>
    <tr>
        <td>Person</td>
        <td>{{ e2p_observer.psnSeqno }}</td>
    </tr>
    <tr>
        <td>Person</td>
        <td>{{ e2p_gatherer.psnSeqno }}</td>
    </tr> #}
    </tbody>
</table>
<table class="observation_comments_view table">
    <tbody>
    <tr>
        <td>Observation description</td>
        <td>{{ observation.eseSeqno.description }}</td>
    </tr>
    <tr>
        <td>Event web comments English</td>
        <td>{{ observation.webcommentsEn }}</td>
    </tr>
    <tr>
        <td>Event web comments Dutch</td>
        <td>{{ observation.webcommentsNl }}</td>
    </tr>
    <tr>
        <td>Event web comments French</td>
        <td>{{ observation.webcommentsFr }}</td>
    </tr>
    </tbody>
</table>


{#{% for ov in observation.values %}
