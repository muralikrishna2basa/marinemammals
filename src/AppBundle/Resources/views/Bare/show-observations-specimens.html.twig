<fieldset id="circumstantial_values" class="no-multi">
    <legend><h4>Circumstantial parameters</h4></legend>
    {% if observation.eseSeqno.spec2events.circumstantialValues|first.vars.data.valueRequired == false %}<p
     id="circumstantialParametersFeedback" class="text-danger">Although the
 circumstantial parameters are not required to be completed when updating an existing observation, we would like
 you to provide this information nonetheless. The level of uncertainty can be expressed in the value
 flag.</p>{% endif %}
    {% for sv in observation.eseSeqno.spec2events.circumstantialValues %}
 <h5>{{ sv.vars.value.pmdName }}</h5>
 <span ><div class="form-inline">Value</span> <span >{{ sv.value }}</span>
 <span >    Value flag</span> <span >{{ sv.valueFlag }}</span></div>
    {% endfor %}
</fieldset>
<fieldset id="measurement_values" class="no-multi">
    <legend><h4>Measurements</h4></legend>
    {% for sv in observation.eseSeqno.spec2events.measurementValues %}
 <h5>{{ sv.vars.value.pmdName }}</h5>
 <span ><div class="form-inline">Value</span> <span >{{ sv.value }}</span>
 <span >    Value flag</span> <span >{{ sv.valueFlag }}</span></div>
    {% endfor %}
</fieldset>

<fieldset id="pathology_values" class="no-multi">
    <legend><h2>External pathology</h2></legend>
    <fieldset>
 <legend><h3>Fresh external lesions</h3></legend>
 <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(0, 9) %}
  {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last}) }}
     {% endfor %}</div>
 <fieldset>
     <legend><h4>Line/net impressions or cuts</h4></legend>
     <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(9, 4) %}
      {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last, 'attr'{'class''form-inline'}}) }}
  {% endfor %}</div>
 </fieldset>
 <fieldset>
     <legend><h4>Scavenger traces</h4></legend>
     <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(13, 2) %}
      {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last, 'attr'{'class''form-inline'}}) }}
  {% endfor %}</div>
 </fieldset>
    </fieldset>
    <fieldset>
 <legend><h3>Healing/healed lesions</h3></legend>
 <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(15, 5) %}
  {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last}) }}
     {% endfor %}</div>
    </fieldset>
    <fieldset>
 <legend><h3>Fishing activities</h3></legend>
 <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(20, 2) %}
  {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last}) }}
     {% endfor %}</div>
    </fieldset>
    <fieldset>
 <legend><h3>Other characteristics</h3></legend>
 <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.pathologyValues|slice(22, 9) %}
  {{ form_row(sv.value, {'label'sv.vars.value.pmdName|split('')|last}) }}
     {% endfor %}</div>
    </fieldset>
</fieldset>

<fieldset id="causeofdeath_values" class="no-multi only-dead">
    <fieldset>
 <legend><h3>Cause of death initial assessment</h3></legend>
 <div class="form-inline">{% for sv in observation.eseSeqno.spec2events.causeOfDeathValues|slice(0, 6) %}
