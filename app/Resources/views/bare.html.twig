<!-- app/Resources/views/base.html.twig -->
<!DOCTYPE html>
<html>
<head>
    {% block head %}
        <meta http-equiv="Content-Type" content="text/html" charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="Marine Mammals of Belgium">
        <meta name="author" content="Thomas Vandenberghe">
        <title>MM: {% block title %}{% endblock %}</title>
        <link rel="shortcut icon"
              href="http://www.odnature.be/assets/nav/logos/logo32x32.ico"/>
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <link rel="stylesheet" type="text/css" media="print"  href="https://fonts.googleapis.com/css?family=PT+Sans:400,400italic,700,700italic|Patua+One|Yanone+Kaffeesatz" />
        <![endif]-->
        {% stylesheets
        'http://www.odnature.be/css/odnature.css'
        '@AppBundle/Resources/public/css/font-awesome.css'
        '@AppBundle/Resources/public/css/mm.css' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
        {% block stylesheets %}
        {% endblock %}
    {% endblock %}
</head>
<body>
{% block container %}
{% endblock %}
{% block footer_container %}
{% endblock %}
{% block javascripts %}
    {% javascripts
    'http://code.jquery.com/jquery-1.11.2.min.js'
    'http://code.jquery.com/ui/1.11.2/jquery-ui.js'
    'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js'
    '@AppBundle/Resources/public/js/global.js' %}
    <script type="application/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

</body>
</html>